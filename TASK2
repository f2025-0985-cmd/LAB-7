#include <iostream>
using namespace std;

int main() {
    int num, power;
     result = 1;

    cout << "Enter number: ";
    cin >> num;
    cout << "Enter power: ";
    cin >> power;

    for(int i = 1; i <= power; i++) {
        result *= num;
    }

    cout << num << "^" << power << " = " << result << endl;
    return 0;
}

